class Node{
    constructor(val){
        this.data = val;
        this.next = null;
        this.prev = null;
    }
}
class Doublylinkedlist{
    constructor(){
        this.head = null
        this.tail = null
        this.length = 0
    }
    push(val){
        var newNode = new Node(val)
        if(this.length === 0){
            this.head = newNode
            this.tail = newNode
        } else{
            this.tail.next = newNode;
            newNode.prev = this.tail;
            this.tail = newNode
        }
        this.length = this.length + 1
        return this.length
    }
    pop(){
        if(!this.head){
            return "no node available"
        }
        var poppedNode = this.tail;
        if(this.length === 1){
            this.head = null;
            this.tail = null;
        } else {
            this.tail = poppedNode.prev;
            this.tail.next = null;
            poppedNode.prev = null;
        }
        this.length--;
        return poppedNode;
    }
}
const ar = new Doublylinkedlist()
